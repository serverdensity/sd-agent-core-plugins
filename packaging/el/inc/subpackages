%package -n sd-agent-activemq
Summary: Server Density Monitoring Agent. activemq plugin
Group: System/Monitoring
Requires: %{name} >= 2.2.2, %{name}-jmx >= 2.2.2
BuildArch: noarch

%description -n sd-agent-activemq
%{longdescription}
This package installs the activemq plugin.

%files -n sd-agent-activemq
%defattr(-,root,root,-)
%config /etc/sd-agent/conf.d/activemq.yaml.example

